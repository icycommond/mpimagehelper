{
  "name": "mpimagehelper",
  "displayName": "mp_image_helper",
  "description": "an extension to make svg development easier",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "mp_image_helper",
      "properties": {
        "mpimagehelper.previewWidth": {
          "type": "number",
          "default": 200,
          "description": "预览图片的宽度."
        },
        "mpimagehelper.languages": {
          "type": "array",
          "default": [
            "html",
            "markdown",
            "vue",
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact"
          ],
          "description": "支持的语言."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "vsce": "vsce",
    "vsix": "yarn vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "vsce": "^2.15.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "image-size": "^1.1.1",
    "node-fetch": "^3.3.2"
  }
}
